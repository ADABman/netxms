# define startup object files
INPUT c:/novell/ndk/libc/imports/libcpre.gcc.o   # clib startup code
INPUT nxagentd_r.o

# Import/Export
IMPORT @c:/novell/ndk/libc/imports/libc.imp
IMPORT @../../libnetxms/netxms.imp
