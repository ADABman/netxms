# common definitions
@../../netware/nlmconv.inc

# define startup object files
INPUT libnxlp_r.o

# Imports/Exports
IMPORT (NETXMS) @../libnetxms/netxms.imp
IMPORT (NXEXPAT) @../libexpat/libexpat/nxexpat.imp
EXPORT (LIBNXLP) @libnxlp.imp

# Entry points
START _LibCPrelude
EXIT _LibCPostlude

# NLM header
OUTPUT libnxlp.nlm
TYPE 0
DESCRIPTION "NetXMS Log Parsing Library"
SCREENNAME "System Console"
FLAG_ON 64

MODULE LIBC, NXEXPAT, NETXMS
