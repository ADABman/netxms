<parser>
	<file>C:\my_log.txt</file>
	<rules>
		<rule>
			<match>.*([A-Za-z]+) failed.*</match>
			<event params="1">14</event>
		</rule>
		<rule>
			<match>.*warning.*</match>
			<event>WARNING_EVENT</event>
		</rule>
		<rule>
			<id>8-10</id>
			<level>0</level>
			<source>.*system.*</source>
			<event>WARNING_EVENT</event>
		</rule>
	</rules>
</parser>
